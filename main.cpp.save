#include <string.h>
#include <stdexcept>

#include "Interfaces.h"
#include "Controladoras.h"
#include "Builders.h"

using namespace std;

int main()
{
//TEste
    Projeto prj;
    Codigo cd;
    cd.setValor("12345678900");
    prj.setCodigo(cd);
    Texto nm;
    nm.setValor("Testando 123");
    prj.setDescricao(nm);
    prj.setNome(nm);
    Matricula mt;
    mt.setValor("ABCD1234");
    prj.SetUsuario(mt);
    ContainerProjeto* ct = ContainerProjeto::getInstancia();
    ct->incluir(prj);
////////////////////////
    // Instanciar Builder.

    BuilderSistema* builder;
    builder = new BuilderSistema();

    // Construir (build) sistema.

    CntrApresentacaoControle* cntrApresentacaoControle;
    cntrApresentacaoControle = builder->construir();

    // Solicitar serviço de apresentacao.

    cntrApresentacaoControle->executar();

    // Destruir builder.

    delete builder;

    return 0;
}
